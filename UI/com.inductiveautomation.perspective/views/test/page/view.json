              "transforms": [
                {
                  "code": "\ttempList \u003d []\n\tfor datum in value.getColumnAsList(0):\n\t\tdate \u003d datum.rsplit(\"_\",1)[-1].split(\"-\")\n\t\ttempList.append(system.date.toMillis(system.date.getDate(int(date[0]),int(date[1])-1, int(date[2]))))\n\treturn tempList",
                  "type": "script"
                }
              ],
              "type": "tag"
            }
          },
          "props.params.month": {
