{
  "custom": {},
  "params": {
    "payload": {
      "column": "",
      "dateTime": "",
      "row": "",
      "tagPath": ""
    }
  },
  "propConfig": {
    "params.payload": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 408,
      "width": 299
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "DateTimePicker"
        },
        "position": {
          "basis": "360px"
        },
        "props": {
          "formattedValue": null
        },
        "type": "ia.input.date-time-picker"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "script": "\tself.view.params.payload.dateTime \u003d self.getSibling(\"DateTimePicker\").props.value\n\tsystem.perspective.sendMessage(\"addDateTime\", self.view.params.payload)\n\tsystem.perspective.closePopup(\"dateTime_\"+str(self.view.params.payload.row)+\"_\"+str(self.view.params.payload.column))"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Button"
        },
        "position": {
          "basis": "48px"
        },
        "propConfig": {
          "props.enabled": {
            "binding": {
              "config": {
                "expression": "{../DateTimePicker.props.value} !\u003d null"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "text": "Add Time"
        },
        "type": "ia.input.button"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "type": "ia.container.flex"
  }
}